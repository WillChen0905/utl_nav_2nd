<launch>
  <node name="ydlidar_node1"  pkg="ydlidar"  type="ydlidar_node" output="screen">
    <param name="port"         type="string" value="/dev/ttyUSB0"/>  
    <param name="baudrate"     type="int"    value="230400"/>
    <param name="frame_id"     type="string" value="laser1"/>
    <param name="angle_fixed"  type="bool"   value="true"/>
    <param name="resolution_fixed"    type="bool"   value="false"/>
    <param name="angle_min"    type="double" value="-115" />
    <param name="angle_max"    type="double" value="180" />
    <param name="range_min"    type="double" value="0.1" />
    <param name="range_max"     type="double" value="10.0" />
    <param name="ignore_array" type="string" value="" />
    <remap from="scan" to="scan1" />
  </node>

   <node name="ydlidar_node2"  pkg="ydlidar"  type="ydlidar_node" output="screen">
    <param name="port"         type="string" value="/dev/ttyUSB1"/>  
    <param name="baudrate"     type="int"    value="230400"/>
    <param name="frame_id"     type="string" value="laser2"/>
    <param name="angle_fixed"  type="bool"   value="true"/>
    <param name="intensities"  type="bool"   value="false"/>
    <param name="angle_min"    type="double" value="-115" />
    <param name="angle_max"    type="double" value="145" />
    <param name="range_min"    type="double" value="0.1" />
    <param name="range_max"    type="double" value="10.0" />
    <param name="ignore_array" type="string" value="" />
    <remap from="scan" to="scan2" />
  </node>
<!--node pkg="tf" type="static_transform_publisher" name="base_link_to_laser4"
    args="0.2245 0.0 0.2 0.12 0.0  0.0 /base_footprint /laser_frame 40" /-->
</launch>
